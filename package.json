  "main": "server.js",
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "apollo-server": "^2.14.1",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "js-base64": "^2.5.2",
    "sequelize": "^5.21.11",
    "sqlite": "^3.0.6"
  },
  "devDependencies": {
    "@babel/node": "^7.10.1",
    "@jest/globals": "^26.0.1",
    "apollo-fetch": "^0.7.0",
